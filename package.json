    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "mockdate": "^3.0.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.1.3",
    "ts-jest": "^24.3.0",
    "typescript": "^3.7.4"
  },
